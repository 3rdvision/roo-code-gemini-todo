# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp/*
!/tmp/.keep

# Ignore database files.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore node_modules directory.
/node_modules

# Ignore yarn error logs.
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore files generated by VS Code.
.vscode/

# Ignore .env file containing environment variables.
.env*
!.env.example

# Ignore OS-specific files
.DS_Store
._*
Thumbs.db

# Ignore coverage reports
/coverage/

# Ignore Rails cache files
/tmp/cache/

# Ignore pidfiles
/tmp/pids/*
!/tmp/pids/.keep

# Ignore session store files
/tmp/sessions/*
!/tmp/sessions/.keep

# Ignore socket files
/tmp/sockets/*
!/tmp/sockets/.keep

# Ignore compiled assets
/public/assets

# Ignore Webpacker/Shakapacker specific files
/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore Turbo Native configuration file
/config/turbo-native.json

# Ignore Byebug history.
.byebug_history

# Ignore System test screenshots.
/tmp/screenshots/

# Ignore Docker specific files
Dockerfile.prod
.dockerignore
docker-compose.yml

# Ignore local secrets
config/credentials/development.key
config/credentials/test.key